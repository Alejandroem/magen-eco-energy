    "version": "0.2.0",
    "configurations": [
        {
            "name": "Magen dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                
                "--dart-define",
                "ENV_NAME=Development",
                "--dart-define",                
                "ENV_SCHEMA=http",
                "--dart-define",
                "ENV_HOST=127.0.0.1:3000",                
            ],
            "args": [
                
            ]
        },
        {
            "name": "Magen prod",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define",
                "ENV_NAME=Production",
                "--dart-define",
                "ENV_SCHEMA=https",
                "--dart-define",
                "ENV_HOST=discountonservices.net",                
            ],
            "args": [
                
            ]
        },
        {
            "name": "magen-eco-energy (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "magen-eco-energy (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}